<% if @stream.live? %>
  <% if params[:tech] && ['dash', 'flash', 'hds', 'hls'].include? params[:tech] %>
    <%= render 'streams/players/hls' %>
  <% else %>
    <% if user_signed_in? %>
      <%= render 'streams/players/' + current_user.settings(:player).tech %>
    <% else %>
      <%= render 'streams/players/hls' %>
    <% end %>
  <% end %>
<% else %>
  <%= render 'streams/players/offline' %>
<% end %>
<script type="text/javascript" charset="utf-8">
  
</script>